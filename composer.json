{
    "name": "AHoroshii/url-shortener-bundle",
    "description": "Url shortener bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": ">=7.1.3",
        "doctrine/orm": "^2.4",
        "hashids/hashids": "^3.0|^4.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.1|^5.0"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AHoroshii\\UrlShortenerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AHoroshii\\UrlShortenerBundle\\Tests\\": "test"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
